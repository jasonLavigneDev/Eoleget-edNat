 <script type='text/javascript'>
           $(function () {
            // 二级菜单
            $('.HNav ul.OneNul li').hover(function () {
                if ($(this).children().hasClass('subNav')) {
                    $(this).children('.subNav').show();
                    $('.HomeKVbg').show();
                }
            }, function () {
                if ($(this).children().hasClass('subNav')) {
                    $(this).children('.subNav').hide();
                    $('.HomeKVbg').hide();
                }
            });
        });
        $('.HomeKVbg').hide();
</script>

